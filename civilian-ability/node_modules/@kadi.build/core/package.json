{
  "name": "@kadi.build/core",
  "version": "0.0.1-alpha.9",
  "description": "A module that is a comprehensive toolkit for developers integrating with the KADI infrastructure.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./types/protocol": {
      "import": "./dist/types/protocol.js",
      "types": "./dist/types/protocol.d.ts"
    },
    "./types/events": {
      "import": "./dist/types/events.js",
      "types": "./dist/types/events.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas/index.js",
      "types": "./dist/schemas/index.d.ts"
    },
    "./messages": {
      "import": "./dist/messages/BrokerMessages.js",
      "types": "./dist/messages/BrokerMessages.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "author": "Corey Clark",
  "contributors": [
    {
      "name": "Kassi Bertrand",
      "url": "https://github.com/kassi-bertrand"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.5.0",
    "debug": "^4.4.1",
    "events": "^3.3.0",
    "ws": "^8.16.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^20.19.11",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
